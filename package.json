    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@mantine/core": "7.17.8",
    "@mantine/dropzone": "7.17.8",
    "@mantine/hooks": "7.17.8",
    "@mantine/notifications": "7.17.8",
    "@tabler/icons-react": "3.35.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "zustand": "4.5.7"
  }
}
